
local Library = loadstring(game:HttpGet("https://reallinen.github.io/Files/Scripts/Linui.lua"))()
local Section = Library:Section("House . Player") -- Creates a section
local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()

local Label = Section:Label({
	Text = "ClockMenu :RP", -- Text/Name of the label
})

OrionLib:MakeNotification({
    Name = "ClockMenu:Free",
    Content = "This Is FREE SCRIPT!",
    Image = "rbxassetid://4483345998",
    Time = 10
})


